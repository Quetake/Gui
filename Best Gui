local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Chiezyy's Hub", "Serpent")

-- tabs
local Games = Window:NewTab("Games")
local Others = Window:NewTab("Others")

--Sections
local GameSection = Games:NewSection("Games")
local OtherSection = Others:NewSection("Others")

--Games
GameSection:NewButton("Natural Disaster Survival", "Free Balloon", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/2dgeneralspam1/scripts-and-stuff/master/scripts/LoadstringUjHI6RQpz2o8", true))()
end)
GameSection:NewButton("Doors", "Darkrai-X", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Darkrai-X/main/Games/Doors"))()
end)
GameSection:NewButton("Arsenal", "PWNERHUB", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Maikderninja/Maikderninja/main/PWNERHUB.lua"))();
end)
GameSection:NewButton("Brookhaven", "Ice Hub", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Ice-Hub-6414", true))()
end)

--Others
OtherSection:NewButton("Netless", " Fix falling bodies when loading animations ", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Quetake/Netless/main/Netless"))()
end)

OtherSection:NewButton("Keyboard V1", "  ", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/jTf7ZtLN"))()
end)

OtherSection:NewButton("Keyboard V2", "  ", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/xSsDaMg2"))()
end)
OtherSection:NewButton("Fly Mobile", " Pc Can Use it", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/QZnf0M5D"))()
end)
